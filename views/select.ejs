<!DOCTYPE html>
<html>
  <head>
		<%- include partial/head.ejs %>
  </head>
  <body>
    <%- include('partial/nav', {title: "Select"}); %>
    <h1>Ride Sharing: <%= title%></h1>
		<table class="table">
			<thead>
				<tr>
					<th scope="col">Driver Email</th>
					<th scope="col">Driver Password</th>
					<th scope="col">Driver Name</th>
				</tr>
			</thead>
			<tbody>
				<% for(var i=0; i<data.length; i++) { %>
				<tr>
					<td><%= data[i].email%></td>
					<td><%= data[i].password%></td>
					<td><%= data[i].name%></td>
				</tr>
				<% } %>
			</tbody>
		</table>
  </body>
  <footer>
    <%- include partial/scripts.ejs %>
  </footer>
</html>
